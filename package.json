{
  "name": "ts-req",
  "version": "0.1.7",
  "description": "light request",
  "author": "junyeong CHOI <98.junyeong@gmail.com>",
  "repository": "https://github.com/junyeongCHOI/ts-req",
  "license": "MIT",
  "module": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run build && terser ./dist/index.js -o ./dist/index.js -m -c  && jest",
    "prepublish": "tsc && terser ./dist/index.js -o ./dist/index.js -m -c"
  },
  "keywords": [
    "ts-req",
    "tsreq",
    "tsReq",
    "axios",
    "fetch",
    "request",
    "xhr"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ]
  }
}
